</head>
<body>
  <div class="navbar-container sticky-top">
    <div class="navTitle">Scholarship Auditions</div>
    <ul class="navbar-list">
      <li><a href="/home">Home</a></li>
      <li><a href="/founder">About</a></li>
      <li><a href="/intern-program">Intern Programs</a></li>
      <li><a href="/contact">Contact</a></li>
  </div>

<script>
  const navbar = document.querySelector('.navbar-container')
  const navbarList = document.querySelector('.navbar-list')

  window.addEventListener('scroll', ()=> {
    if(window.scrollY > 50) {
      navbar.classList.add('navbar-container-scrolled')
      navbarList.classList.add('navbarList-scrolled')
      navbarList.classList.remove('navbar-list')
    } else if (window.scrollY <=50 ) {
      navbar.classList.remove('navbar-container-scrolled')
      navbarList.classList.remove('navbarList-scrolled')
      navbarList.classList.add('navbar-list')
    }
  })
</script>
</body>
</html>
